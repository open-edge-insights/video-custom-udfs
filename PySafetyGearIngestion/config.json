{
    "config": {
        "encoding": {
            "level": 95,
            "type": "jpeg"
        },
        "ingestor": {
            "loop_video": true,
            "pipeline": "./test_videos/Safety_Full_Hat_and_Vest.avi",
            "poll_interval": 0.2,
            "queue_size": 10,
            "type": "opencv"
