{
    "config": {
        "encoding": {
            "level": 95,
            "type": "jpeg"
        },
        "ingestor": {
            "loop_video": true,
            "pipeline": "./test_videos/Safety_Full_Hat_and_Vest.avi",
            "poll_interval": 0.2,
            "queue_size": 10,
            "type": "opencv"
        },
        "sw_trigger": {
            "init_state": "running"
        },
        "max_workers": 4,
          "udfs": [
             {
                "name": "dummy",
                "type": "python"
             }
           ]
    },
    "interfaces": {
        "Servers": [
            {
                "Name": "py_safety_gear",
                "Type": "zmq_tcp",
                "EndPoint": "127.0.0.1:66019",
                "AllowedClients": [
                    "*"
                ]
            }
        ],
        "Publishers": [
            {
                "Name": "default",
                "Type": "zmq_ipc",
                "EndPoint": "/EII/sockets",
                "Topics": [
                    "py_safety_gear_stream"
                ],
                "AllowedClients": [
                    "*"
                ]
            }
        ]
    }
}
