{
    "Module": {
        "Name": "GVASGI",
        "Description": "GVASafetyGearIngestion manifest",
        "SchemaVersion": "0.2",
        "ManifestFile": "${idx:gva_safety_gear_ingestion:$EIS_VERSION}",
        "Resources": {
            "CPU": 100,
            "MemoryMB": 100
        },
        "ExecutionEnv": {
            "AppName": "GVASafetyGearIngestion",
            "ETCD_ENDPOINT": "${datastore.endpoint}",
            "CONFIGMGR_CACERT": "${databucket.cacert}",
            "CONFIGMGR_CERT": "${databucket.cert}",
            "CONFIGMGR_KEY": "${databucket.key}",
            "ETCD_PREFIX": "$ETCD_PREFIX",
            "GENICAM": "Basler",
            "DEV_MODE": "$DEV_MODE",
            "PROFILING_MODE": "false",
            "PUBLISHER_TYPE": "zmq_tcp",
            "PUBLISHER_ENDPOINT": "0.0.0.0:${ep.op-gva.localport}",
            "SERVER_ENDPOINT": "0.0.0.0:${ep.op-gva-sw-tr.localport}"
        },
        "Endpoints": [
            {
                "Name": "op-gva-sw-tr",
                "Endtype": "server",
                "DataType": "messages",
                "Link": "gva-sw-tr"
            },
            {
                "Name": "op-gva",
                "Endtype": "server",
                "DataType": "messages",
                "Link": "gva-link"
            }
        ]
    }
}
